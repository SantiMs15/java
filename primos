import java.util.Scanner;


Scanner num = new Scanner (System.in);
        int numero;
        int a = 0;
        System.out.println("Numero: ");
        numero = num.nextInt();
        
        for (int i = 1; i <= numero; i++) {
            if (numero%i == 0){
                a++;
            }
        }
        if (a==2){
            System.out.println("Primo");
        }else{
            System.out.println("No es primo");
        }
    }
}
